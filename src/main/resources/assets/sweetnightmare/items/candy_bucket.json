{
  "type": "minecraft:select",
  "property": "minecraft:custom_model_data",
  "cases": [
    {
      "when": "low",
      "model": {
        "type": "minecraft:model",
        "model": "sweetnightmare:item/candy_bucket_low"
      }
    },
    {
      "when": "half",
      "model": {
        "type": "minecraft:model",
        "model": "sweetnightmare:item/candy_bucket_half"
      }
    },
    {
      "when": "full",
      "model": {
        "type": "minecraft:model",
        "model": "sweetnightmare:item/candy_bucket_full"
      }
    },
    {
      "when": "default",
      "model": {
        "type": "minecraft:model",
        "model": "sweetnightmare:item/candy_bucket"
      }
    }
  ],
  "fallback": {
    "type": "minecraft:model",
    "model": "sweetnightmare:item/candy_bucket"
  }
}